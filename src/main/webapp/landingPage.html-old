<body id="body" class="no-js" class="preload-01 preload-02">
	<div id="LandingPage">
		<div id="content">
			<div id="header" onclick="openCreateAccountDialog()">
				<div id="titles">
					<div class="title">
						<div id="logo"></div>
						<span>start watching movies instantly!</span>
					</div>
					<div class="subtitle" id="movieTitle">HD Streaming(720)</div>
				</div>
				<div class="download">
					<button class="greenBtn">
						<span class="title">DOWNLOAD</span> <span class="subtitle">TO
							YOUR COMPUTER</span>
					</button>
					<div id="stars">
						<i class="material-icons">star</i><i class="material-icons">star</i><i
							class="material-icons">star</i><i class="material-icons">star</i><i
							class="material-icons">star</i>
					</div>
				</div>
			</div>
			<div class="LP_TV_TopBar">
				<div class="LP_TV_TopMenu">
					<span>Home</span> <span>New episodes</span> <span>Most
						Popular</span> <span>genres</span> <span>my watchlist</span>
				</div>
			</div>
			<div class="playerContainer" onclick="openCreateAccountDialog()">
				<div class="width ratio16-9">
					<div class="content">
						<div class="loading" id="loading">
							<div id="label">
								<div class="loaderImg"></div>
								<div id="loaderPercentage"></div>
								<div class="loaderBigTitle">CONNECTING TO SATELITE</div>
								<div class="loaderSmallTitle">Stream No.16</div>


							</div>
							<div class="radial-progress" data-progress="0"
								id="radial-progress">
								<div class="circle">
									<div class="loaderMask full">
										<div class="fill"></div>
									</div>
									<div class="loaderMask half">
										<div class="fill"></div>
										<div class="fill fix"></div>
									</div>
									<div class="shadow"></div>
								</div>
								<div class="inset">
									<div class="percentage">
										<div class="numbers">
											<span>-</span><span>0%</span><span>1%</span><span>2%</span><span>3%</span><span>4%</span><span>5%</span><span>6%</span><span>7%</span><span>8%</span><span>9%</span><span>10%</span><span>11%</span><span>12%</span><span>13%</span><span>14%</span><span>15%</span><span>16%</span><span>17%</span><span>18%</span><span>19%</span><span>20%</span><span>21%</span><span>22%</span><span>23%</span><span>24%</span><span>25%</span><span>26%</span><span>27%</span><span>28%</span><span>29%</span><span>30%</span><span>31%</span><span>32%</span><span>33%</span><span>34%</span><span>35%</span><span>36%</span><span>37%</span><span>38%</span><span>39%</span><span>40%</span><span>41%</span><span>42%</span><span>43%</span><span>44%</span><span>45%</span><span>46%</span><span>47%</span><span>48%</span><span>49%</span><span>50%</span><span>51%</span><span>52%</span><span>53%</span><span>54%</span><span>55%</span><span>56%</span><span>57%</span><span>58%</span><span>59%</span><span>60%</span><span>61%</span><span>62%</span><span>63%</span><span>64%</span><span>65%</span><span>66%</span><span>67%</span><span>68%</span><span>69%</span><span>70%</span><span>71%</span><span>72%</span><span>73%</span><span>74%</span><span>75%</span><span>76%</span><span>77%</span><span>78%</span><span>79%</span><span>80%</span><span>81%</span><span>82%</span><span>83%</span><span>84%</span><span>85%</span><span>86%</span><span>87%</span><span>88%</span><span>89%</span><span>90%</span><span>91%</span><span>92%</span><span>93%</span><span>94%</span><span>95%</span><span>96%</span><span>97%</span><span>98%</span><span>99%</span><span>100%</span>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="screen" id="screen">
							<div class="movie"></div>
							<div class="movieTimer" onclick="openCreateAccountDialog()">
								<div id="circularG"></div>
								<span id="loadSpan">Loading...</span>
								<div class="contentBar" id="contentBar">
									<span class="expand "></span> <span class="expand expandRed"></span>
								</div>
							</div>
						</div>
						<div class="controllers" onclick="openCreateAccountDialog()">
							<div class="movieTimer" onclick="openCreateAccountDialog()">
								<div id="circularG"></div>
								<span id="loadSpan">Loading...</span>
								<div class="contentBar" id="contentBar">
									<span class="expand "></span> <span class="expand expandRed"></span>
								</div>
							</div>

						</div>
					</div>
				</div>
			</div>
			<div id="languages" onclick="openCreateAccountDialog()"></div>
			<div class="tvContent">
				<div class="headerTvContent">
					<span>Links available:</span>
				</div>
				<table onclick="openCreateAccountDialog()">
					<tr>
						<td><span class="link" id="link1"></span></td>
						<td class="watchBtn"></td>
						<td class="tableStarsWrapper"><span class="star"></span> <span
							class="star"></span> <span class="star"></span> <span
							class="star"></span> <span class="starEmpty"></span> <span
							class="votes">2563 Votes</span></td>
						<td class="voteBtn"></td>
					</tr>
					<tr>
						<td><span class="link" id="link2"></span></td>
						<td class="watchBtn"></td>
						<td class="tableStarsWrapper"><span class="star"></span> <span
							class="star"></span> <span class="star"></span> <span
							class="star"></span> <span class="star"></span> <span
							class="votes">2563 Votes</span></td>
						<td class="voteBtn"></td>
					</tr>
					<tr>
						<td><span class="link" id="link3"></span></td>
						<td class="watchBtn"></td>
						<td class="tableStarsWrapper"><span class="star"></span> <span
							class="star"></span> <span class="star"></span> <span
							class="star"></span> <span class="starEmpty"></span> <span
							class="votes">2563 Votes</span></td>
						<td class="voteBtn"></td>
					</tr>
					<tr>
						<td><span class="link" id="link4"></span></td>
						<td class="watchBtn"></td>
						<td class="tableStarsWrapper"><span class="star"></span> <span
							class="star"></span> <span class="star"></span> <span
							class="star"></span> <span class="starEmpty"></span> <span
							class="votes">2563 Votes</span></td>
						<td class="voteBtn"></td>
					</tr>
					<tr>
						<td><span class="link" id="link5"></span></td>
						<td class="watchBtn"></td>
						<td class="tableStarsWrapper"><span class="star"></span> <span
							class="star"></span> <span class="star"></span> <span
							class="starEmpty"></span> <span class="starEmpty"></span> <span
							class="votes">2563 Votes</span></td>
						<td class="voteBtn"></td>
					</tr>
				</table>
			</div>
			<div class="mask" id="mask"></div>
			<div id="loadingMask"></div>
			<div id="CreateAccount">
				<div class="registrationWrapper">
					<div class="dialogMask"></div>
					<div id="registration">
						<p>Only registered friends can watch all the movies!</p>
						<p class="enlargedText">
							you must create a <span class="green">FREE</span> account to <span
								class="green">STREAM</span> this movie
						</p>
						<input type="email" id="email" placeholder="E-mail"
							onkeydown=checkEnter(event,this)> <input type="password"
							id="psw" maxlength="12" pattern=".{5,12}" required
							placeholder="Password" onkeydown=checkEnter(event,this)>
						<input type="Password" id="psw2" maxlength="12" pattern=".{5,12}"
							required placeholder="Confirm Password"
							onkeydown=checkEnter(event,this)>
						<button class="greenBtn" onclick="CreateAccountValidation()">CREATE
							MY FREE ACCOUNT NOW</button>
					</div>
				</div>
			</div>
		</div>
